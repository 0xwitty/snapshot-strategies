{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. UNI"],
          "maxLength": 16
        },
        "addresses": {
          "title": "Addresses",
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["addresses"],
      "additionalProperties": false
    }
  }
}
